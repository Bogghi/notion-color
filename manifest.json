{
    "manifest_version": 2,
    "name": "notion_color",
    "description": "change default color in notion",
    "version": "1.0",
    "content_scripts": [{
        "matches": ["https://www.notion.so/*"],
        "css": ["main.css"]
    }],
    "background": {
        "scripts": ["./background.js"]
    },
    "options_page": "./option.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "https://www.notion.so/*"
    ]
}